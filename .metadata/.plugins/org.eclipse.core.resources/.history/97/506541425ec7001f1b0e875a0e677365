package LogicalQ;

public class Prime_CheckINGivenRange {

	public static void main(String[] args) {
	
		int start =2;
		int end =10;
		
		int count;
		
		for(int i=start; i<=end; i++ )
		{
			count =0;
			for(int j=1;j<=i;j++)
			{
				int res = i%j;
				if(res==0)
				{
					count++;
				}
			}
			
			if(count==2)
			{
				System.out.println("given number "+ i +" is prime");
			}
		}

	}

}
