package LogicalQ;

public class Prime_CheckGivenNumPrimeOrNot {

	public static void main(String[] args) {
		
		int num =13;
		int count;
		for(int i=2;i<=num;i++)
		{
			count=0;
			
			if(i%num==0)
			{
				count++;
			}
		}
		if(count==2)
		{
			System.out.println("given number "+ num +" is prime");
		}
		else 
		{
			System.out.println("given number "+ num +" is not a prime");
		}
	}

}
